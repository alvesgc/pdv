generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id        Int      @id @default(autoincrement())
  code    Int
  bar_code String
  name      String
  price     Float
  quantity  Int @default(0)
  active    Boolean @default(true)
  createdAt DateTime @default(now())
}
